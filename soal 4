program pascal;
uses crt;

var
    jalan,no,kota,kodepos :string;

Begin
    clrscr;
    write('Jalan:');
    readln(jalan);
    write('No:');
    readln(no);
    write('Kota:');
    readln(kota);
    write('Kode pos:');
    readln(kodepos);
    writeln('Alamat: Jalan ',jalan,' No.',no,' ',kota,' ',kodepos);
    
end.
